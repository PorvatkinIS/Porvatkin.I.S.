age = int(input("Введите возраст:"))
name = input("Введите имя:")

if age > 0 and age < 75:
    if name != "Иван":
        if age >= 16:
            print("Поздравляем, вы поступили в ВГУИТ!")
        else:
            print("Сначала нужно окончить школу!")
            print("Вам еще", 16 - age, "лет учиться в школе.")
    else:
        print("Извините, поступающего зовут Иван.")